            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="page-header">
                        <h3 class="page-title"> Deleted Category Details </h3>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/admin/deleteCategory"> Deleted Category</a></li>
                                <li class="breadcrumb-item active" aria-current="page"> Deleted Category Details</li>
                            </ol>
                        </nav>
                    </div>
                    <div class="col-lg-12 grid-margin stretch-card">
                        <div class="card">
                            <div class="card-body">
                            <div class="d-flex justify-content-between align-items-center">
                                <div>
                                <h4 class="card-title"> Deleted Category Details</h4>
                                <p class="card-description"><code>Kiona Deleted Category</code></p>
                                </div>
                                <div class="d-flex align-items-center">
                                <a href="/admin/category">
                                    <button type="button" class="btn badge badge-info btn-md" >Back <i class="mdi mdi-backburger"></i> </button>
                                </a>
                                <div style="width: 20px;"></div>
                                
                                </div>
                            </div>
                            <table class="table table-bordered">
                                <thead>
                                <tr>
                                    <th> No </th>
                                    <th> Image </th>
                                    <th> Category Nmae </th>
                                    <th> Description </th>
                                    <th> Re-Store </th>
                                    <th> Delete </th>
                                </tr>
                                </thead>
                                <tbody>
                                <% if (category.length > 0) { %>
                                    <% for (let i = 0; i < category.length; i++) { %>
                                    <tr>
                                        <td><%= i + 1 %></td>
                                        <td><img src="Photos/category/<%= category[i].image %>" class="mr-2" alt="Image of <%= category[i].name %>"></td>
                                        <td><%= category[i].name %></td>
                                        <td><%= category[i].description %></td>
                                        <td>
                                        <button type="button" class=" btn badge btn-gradient-success " onclick="restoreCategory('<%= category[i]._id %>')" aria-label="restore Category" >
                                            Re-store <i class="mdi mdi-backup-restore" style="font-size: 15px; width: 15px; height: 15px; color: white;"></i>
                                        </button>
                                        </td>
                                        <td>
                                        <button type="button" class=" btn badge badge-danger" onclick="Delete('<%= category[i]._id %>')" aria-label="Delete Category">
                                            Delete <i class="mdi mdi-delete" style="font-size: 15px; width: 15px; height: 15px; color: white;"></i>
                                        </button>
                                        </td>
                                    </tr>
                                    <% } %>
                                <% } else { %>
                                    <tr>
                                    <td colspan="6">Categories not found</td>
                                    </tr>
                                <% } %>
                                </tbody>
                            </table>
                            </div>
                        </div>
