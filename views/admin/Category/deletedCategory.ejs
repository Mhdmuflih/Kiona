        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

        <script>

            async function restoreCategory(id) {
                const responce = await axios.post('/admin/category/restore', { id })
                if (responce.data.success) {
                    await Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: responce.data.message
                    })
                    window.location.href = '/admin/deleteCategory';
                } else {
                    await Swal.fire({
                        icon: 'error',
                        title: 'error',
                        text: responce.data.message
                    })
                    window.location.href = '/admin/category';
                }
            }

            async function Delete(id) {
                const responce = await axios.post('/admin/category/deleted', { id })
                if (responce.data.success) {
                    await Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: responce.data.message
                    })
                    window.location.href = '/admin/deleteCategory';
                } else {
                    await Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: responce.data.message
                    })
                    window.location.href = '/admin/category';
                }
            }


        </script>



