        <div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header">
              <h3 class="page-title">  All Ordered Details </h3>
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="/admin/order"> Ordered Details </a></li>
                  <li class="breadcrumb-item active" aria-current="page">All Ordered Details</li>
                </ol>
              </nav>
            </div>
            <div class="row">

              <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title"> Ordered Details </h4>
                    <p class="card-description"> <code> Kiona Ordered Details </code>
                    </p>
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th> Sl.No </th>
                          <th> User </th>
                          <th> Email </th>
                          <th> Payment Method </th>
                          <th> Ordered Date </th>
                          <th> Details </th>
                        </tr>
                      </thead>
                      <tbody>
                        <% if(order.length>0){ %>
                          <% for(let i=0 ; i<order.length ; i++){ %>
                            <tr>
                              <td><%= i+1 %></td>
                              <td class="py-1">
                                <img src="/Photos/userImages/<%= users[i].image %>" alt="image" /> <span> <%= users[i].name %> </span>
                              </td>
                              <td> <%= users[i].email %> </td>
                              <td style="color: green;" >
                                <%= order[i].paymentMethod %>
                              </td>
                              <td>  <%= order[i].orderDate.toDateString() %> </td>
                              <td> <button type="button" class="btn btn-gradient-warning btn-sm" onclick="details('<%= order[i]._id %>')" > Details </button> </td>
                            </tr>
                          <% } %>
                        <% }  else { %>
                          <tr>
                            <td colspan="6">Order not fount</td>
                          </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
